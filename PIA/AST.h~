#include "vector"

typedef enum {FLOAT_CONST, FIELD_IN, FLIELD_OUT, LOCAL_VARIABLE, INPUT_VARIABLE} AST_NODE_TYPE;
typedef enum {REG_SINGLE, REG_PAIR, REG_QUAD} OPERAND_SIZE;
typedef enum {ADD, MUL, ADD2, MUL2, ADD4, MUL4} OPERATOR_TYPE;
typedef enum {FLOAT, DOUBLE, INT} DATA_TYPE;

using namespace std;
class ASTNode {
 public:
 OPERAND_SIZE operand_size;
 OPERATOR_TYPE operator_type;
 
 string destination;
 
 vector<ASTNode*> children;
};

class SemanticTreeNode {
 public:
  string dest_id;
  string op;
  vector<string> children;
};



 
     
